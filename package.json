{
  "name": "questioner",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:emrys8/Questioner.git",
  "author": "Micah Akpan <mickey4sure2k7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev:start": "nodemon --require @babel/register ./server/bin/www",
    "start": "node --require @babel/register ./server/bin/www",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js ./server",
    "test": "NODE_ENV=test nyc --reporter=text mocha --require @babel/register --recursive ./server/test/**/** --timeout 5000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gulp": "3.9.1",
    "gulp-mocha": "^6.0.0",
    "gulp-shell": "^0.6.5",
    "gulplog": "^1.0.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "sinon": "^7.2.2",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "joi": "^14.3.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "pg": "^7.7.1",
    "winston": "^3.1.0"
  },
  "engines": {
    "node": "10.x"
  },
  "nyc": {
    "exclude": "**/**/*.spec.js"
  }
}
